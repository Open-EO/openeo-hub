{
    "dbUrl": "mongodb://localhost:27017",
    "dbName": "openeohub",
    "backends": {
        "Google Earth Engine": "https://earthengine.openeo.org/",
        "EURAC WCPS": "https://openeo.eurac.edu/",
        "VITO GeoPySpark": "https://openeo.vito.be/",
        "EODC OpenShift": "https://openeo.eodc.eu/",
        "mundialis GRASS GIS (Actinia)": "https://openeo.mundialis.de/",
